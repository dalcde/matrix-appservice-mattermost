{
  "name": "matrix-appservice-mattermost",
  "version": "0.1.2",
  "description": "",
  "repository": "https://github.com/dalcde/matrix-appservice-mattermost",
  "main": "index.js",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "preinstall": "git config core.hooksPath .githooks",
    "build": "tsc",
    "test": "npm run lint && npm run build && tape build{,/**}/*.test.js",
    "lint": "prettier -c . && eslint --max-warnings=0 '*/**/*.ts'",
    "fmt": "prettier --write .",
    "fix": "eslint '*/**/*.ts' --fix"
  },
  "author": "Dexter Chua",
  "license": "MIT",
  "dependencies": {
    "@types/node-fetch": "^2.5.7",
    "bufferutil": "^4.0.1",
    "form-data": "^3.0.0",
    "js-yaml": "^3.14.0",
    "loglevel": "^1.6.8",
    "marked": "^1.1.1",
    "matrix-appservice-bridge": "^1.13.2",
    "node-fetch": "^2.6.0",
    "pg": "^8.3.0",
    "turndown-performance-fixed": "^6.0.1",
    "typeorm": "^0.2.25",
    "utf-8-validate": "^5.0.2",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "@types/node": "^14.6.0",
    "@types/tape": "^4.13.0",
    "@typescript-eslint/eslint-plugin": "^3.10.0",
    "@typescript-eslint/parser": "^3.10.0",
    "eslint": "^7.7.0",
    "prettier": "^2.0.5",
    "sqlite3": "^5.0.0",
    "tape": "^5.0.1",
    "typescript": "^4.0.0"
  },
  "renovate": {
    "extends": [
      "config:base"
    ],
    "rebaseWhen": "behind-base-branch",
    "rangeStrategy": "update-lockfile",
    "packageRules": [
      {
        "packageNames": [
          "node"
        ],
        "enabled": false
      }
    ]
  }
